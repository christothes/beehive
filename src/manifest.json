{
    "name": "BeeHive",
    "description": "Spelling Bee Companion",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "https://*.nytimes.com/*/spelling-bee"
            ],
            "css": [
                "beehive.css"
            ],
            "js": [
                "content-script.js"
            ]
        }
    ],
    "icons": {
        "16": "spelling-bee-card-icon.png",
        "32": "spelling-bee-card-icon.png",
        "48": "spelling-bee-card-icon.png",
        "128": "spelling-bee-card-icon.png"
    },
    "permissions": [
    ]
}